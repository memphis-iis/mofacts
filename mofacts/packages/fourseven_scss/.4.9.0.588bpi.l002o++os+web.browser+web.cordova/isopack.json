{
  "isopack-2": {
    "name": "fourseven:scss",
    "summary": "Style with attitude. Sass and SCSS support for Meteor.js.",
    "version": "4.9.0",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova.json"
      }
    ],
    "plugins": [
      {
        "name": "compileScssBatch",
        "arch": "os",
        "path": "plugin.compileScssBatch.os/program.json"
      }
    ]
  },
  "isopack-1": {
    "name": "fourseven:scss",
    "summary": "Style with attitude. Sass and SCSS support for Meteor.js.",
    "version": "4.9.0",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os-legacy.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser-legacy.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova-legacy.json"
      }
    ],
    "plugins": [
      {
        "name": "compileScssBatch",
        "arch": "os",
        "path": "plugin.compileScssBatch.os/program.json"
      }
    ]
  }
}